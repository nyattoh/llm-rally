{
  "chatgpt": {
    "name": "ChatGPT",
    "url": "https://chatgpt.com/",
    "selectors": {
      "input": "#prompt-textarea, textarea[placeholder*='Message']",
      "sendButton": "button[data-testid='send-button']",
      "lastMessage": "[data-message-author-role='assistant']",
      "stopButton": "button[aria-label='Stop generating'], button:has-text('Stop generating')"
    }
  },
  "grok": {
    "name": "Grok",
    "url": "https://grok.com/",
    "selectors": {
      "input": "div.ProseMirror, div[contenteditable='true']",
      "sendButton": "button[type='submit'], button[aria-label*='送信'], button[aria-label*='Send']",
      "lastMessage": ".items-start .message-bubble",
      "stopButton": "button:has-text('Stop'), button[aria-label*='Stop'], button[data-testid='stop-button']"
    }
  },
  "claude": {
    "name": "Claude",
    "url": "https://claude.ai/",
    "selectors": {
      "input": "fieldset div[contenteditable='true'], div[contenteditable='true']",
      "lastMessage": "[data-is-streaming]",
      "stopButton": "button:has-text('Stop')"
    }
  },
  "gemini": {
    "name": "Gemini",
    "url": "https://gemini.google.com/",
    "selectors": {
      "input": "rich-textarea div.ql-editor, div.ql-editor.textarea, rich-textarea [contenteditable='true']",
      "sendButton": ".send-button, button[aria-label*='送信'], button[aria-label*='Send']",
      "lastMessage": "model-response .model-response-text, model-response, .message-content",
      "stopButton": "button[aria-label*='Stop'], button:has-text('Stop')"
    }
  }
}