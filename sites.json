{
  "chatgpt": {
    "name": "ChatGPT",
    "url": "https://chatgpt.com/",
    "selectors": {
      "input": "#prompt-textarea, textarea[placeholder*='Message']",
      "sendButton": "button[data-testid='send-button']",
      "lastMessage": "[data-message-author-role='assistant']:last-of-type",
      "stopButton": "button[aria-label='Stop generating'], button:has-text('Stop generating')"
    }
  },
  "grok": {
    "name": "Grok",
    "url": "https://grok.com/",
    "selectors": {
      "input": "div[contenteditable='true'], div[role='textbox'], textarea",
      "sendButton": "button:has-text('Send'), button[aria-label*='Send'], button[type='submit']",
      "lastMessage": "div.prose:last-of-type, [role='article']:last-of-type, .message:last-of-type",
      "stopButton": "button[aria-label*='Stop']"
    }
  },
  "claude": {
    "name": "Claude",
    "url": "https://claude.ai/",
    "selectors": {
      "input": "div[contenteditable='true'], textarea",
      "sendButton": "button[aria-label='Send Message']",
      "lastMessage": "div[data-is-streaming='false']:last-of-type",
      "stopButton": "button:has-text('Stop')"
    }
  },
  "gemini": {
    "name": "Gemini",
    "url": "https://gemini.google.com/",
    "selectors": {
      "input": "rich-textarea[placeholder*='Enter'], textarea",
      "sendButton": "button[aria-label*='Send']",
      "lastMessage": "message-content:last-of-type",
      "stopButton": "button[aria-label='Stop generating']"
    }
  }
}