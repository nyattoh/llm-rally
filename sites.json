{
  "chatgpt": {
    "name": "ChatGPT",
    "url": "https://chatgpt.com/",
    "selectors": {
      "input": "#prompt-textarea, textarea[placeholder*='Message']",
      "lastMessage": "[data-message-author-role='assistant']",
      "stopButton": "button[aria-label='Stop generating'], button:has-text('Stop generating')"
    }
  },
  "grok": {
    "name": "Grok",
    "url": "https://grok.com/",
    "selectors": {
      "input": "textarea, div[contenteditable='true']",
      "lastMessage": "div.prose, .message-content",
      "stopButton": "button[aria-label*='Stop']"
    }
  },
  "claude": {
    "name": "Claude",
    "url": "https://claude.ai/",
    "selectors": {
      "input": "fieldset div[contenteditable='true'], div[contenteditable='true']",
      "lastMessage": "[data-is-streaming]",
      "stopButton": "button:has-text('Stop')"
    }
  },
  "gemini": {
    "name": "Gemini",
    "url": "https://gemini.google.com/",
    "selectors": {
      "input": "div[contenteditable='true'], div.ql-editor.textarea, rich-textarea",
      "sendButton": "button[aria-label*='Send Message'], button[aria-label*='メッセージを送信'], .send-button",
      "lastMessage": "model-response .model-response-text, model-response, .message-content",
      "stopButton": "button[aria-label*='Stop'], button:has-text('Stop')"
    }
  }
}